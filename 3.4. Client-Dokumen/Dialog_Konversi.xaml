        mc:Ignorable="d"
        ResizeMode="NoResize"
        Icon="/Akhi Okhee;component/0. Assets/Ikon-02.png"
        WindowStartupLocation="CenterScreen"
        Background="#CDD7D6"
        Title="Konversi" Height="266.419" Width="440">
    <Grid Margin="0,0,0,0">
        <Label Content="Jenis Konversi:" HorizontalAlignment="Center" Height="23" Margin="169,2,173,0" VerticalAlignment="Top" Width="92" FontWeight="Bold"/>
        <Label Content="Awal :" HorizontalAlignment="Left" Height="24" Margin="91,52,0,0" VerticalAlignment="Top" Width="42"/>
        <Label Content="Akhir :" HorizontalAlignment="Left" Height="24" Margin="298,52,0,0" VerticalAlignment="Top" Width="42"/>
        <ComboBox VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Height="28" Name="jenis" HorizontalAlignment="Left" Margin="10,28,0,0" VerticalAlignment="Top" Width="410"/>
        <TextBox Name="awal" HorizontalAlignment="Left" Height="61" Margin="10,80,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="200" FontSize="40" PreviewTextInput="DoubleValidationTextBox" />
